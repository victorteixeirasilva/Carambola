//SELECT PARA CONSULTA DE TODOS OS PRODUTOS DE MESMA CATEGORIA
SELECT
*
FROM TB_PRODUTOS PRO
  JOIN TB_CATEGORIAS CATE ON PRO.PRO_IDCATE = CATE.CATE_IDCATE
  WHERE CATE.CATE_IDCATE = '1';
  
ALTER SEQUENCE <nome_da_sequencia> RESTART WITH <valor_inicial>;